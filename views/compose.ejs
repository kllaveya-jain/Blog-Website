<%- include("./partials/header"); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
   <label for="title"><strong>Title</strong></label><br>
   <input type="text" name="title" class="form-control" id="title"><br>
   <label for="post"><strong>Post</strong></label><br>
   <textarea name="post" cols="30" rows="5" class="form-control" id="post"></textarea><br>
   <button type="submit" value="publish" class="btn btn-primary" data-bs-toggle="button">Publish</button>
</form>
