<%- include("./partials/header"); -%>
<h1>Home</h1>
<p><%= content %></p>
<% posts.forEach((post) => { %>
   <h3><%= post.title %></h3>
   <p class="postBody"><%= post.content.substring(0, 100) + "... " %>
      <a href="./posts/<%= post._id %>">Read More</a>
   </p>
<% }); %>
<%- include("./partials/footer"); -%>